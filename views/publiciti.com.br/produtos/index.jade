extends ../layout.jade

block conteudo
  // Page heading two starts
  .page-heading-two
    .container
      h2
        | Smart Phones
        span Something goes here
      | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
      .breads
        | Home /
        a(href='#') Page
        |  /
        a(href='#') Sub Page
      | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
      .clearfix
  // Page heading two ends
  // Shopping Items
  .container
    .ecommerce
      .row
        .col-md-9.col-sm-8.col-md-push-3.col-sm-push-4
          // Shopping items content
          .shopping-content
            // Block Title
            h4
              i.fa.fa-laptop.color
              | &nbsp; Phones&nbsp;
              small Quine Poor Squamo
            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
            hr
            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
            .row
              each produto in produtos
                  if produto.imagem
                      .col-md-4.col-sm-6
                        // Shopping items
                        .shopping-item
                          // Image
                          a(href='/produtos/#{produto._id}')
                            img.img-responsive(src='#{produto.imagem.url}', alt='')
                          // Shopping item name / Heading
                          h4
                            a(href='/produtos/#{produto._id}') #{produto.titulo}
                            span.color.pull-right R$ #{produto.valor}
                          | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                          .clearfix
                          // Buy now button
                          .visible-xs
                            a.btn.btn-color.btn-sm(href='#') Comprar
                          // Shopping item hover block & link
                          .item-hover.bg-color.hidden-xs
                            a(href='#') Add to cart
                      | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;

            // Pagination
            ul.pagination
              li.disabled
                a(href='#') &laquo;
              | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
              li.active
                a(href='#')
                  | 1
                  span.sr-only (current)
              | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
              li
                a(href='#') 2
              | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
              li
                a(href='#') 3
              | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
              li
                a(href='#') 4
              | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
              li
                a(href='#') 5
              | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
              li
                a(href='#') &raquo;
            // Pagination end
        | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
        .col-md-3.col-sm-4.col-md-pull-9.col-sm-pull-8
